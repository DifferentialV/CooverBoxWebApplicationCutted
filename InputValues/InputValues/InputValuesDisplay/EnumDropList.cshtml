@model dynamic
@{
    Layout = null;

}

@if (Model.GetValue(ViewBag._model) != null && (Model.GetValue(ViewBag._model) != 0 || Model.DisplayNullVisible  == false))
{
    <div class="lead row">
        <p class="col-xs-12 col-md-4">@($"{Model.DisplayName}")</p>
        <p class="col-xs-12 col-md-8 text-white">@($"{Model.GetValue(ViewBag._model)}".Replace("_"," "))</p>
    </div>
}