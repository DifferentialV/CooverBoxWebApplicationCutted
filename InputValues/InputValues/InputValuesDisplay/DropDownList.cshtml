@model CooverBoxWebApplication.InputValues.InputValuesInfo.DropDownListInputValue
@{
    Layout = null;

}

@if (!string.IsNullOrEmpty(Model.GetValue(ViewBag._model)))
{
    <div class="lead row">
        <p class="col-xs-12 col-md-4">@($"{Model.DisplayName}")</p>
        <p class="col-xs-12 col-md-8 text-white">@($"{Model.GetValue(ViewBag._model)}")</p>
    </div>
}